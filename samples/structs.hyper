struct data_object
{
    name: string;
    level: float;
}

function data_object.to_string() -> string
{
    return "[name: " + name + ", level: " + level.to_string() + "]";
}

function main() -> int32
{
    first_object: data_object = ("First Object", 1.0f);
    first_object.to_string();

    second_object: mutable data_object;
    second_object.level = 2.0f;
    second_object.to_string();
}
