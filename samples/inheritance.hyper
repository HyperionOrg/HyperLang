import sys.io;

struct object
{
}

struct vehicle
{
    name: string;
}

virtual function vehicle.print() -> void
{
    sys.print("[name: " + name + "]");
}

struct car <- vehicle, object
{
    size: uint32
}

override function car.print() -> void
{
    sys.print("[name: "+ name + ", size: " + size + "]");
}

function main() -> int32
{
    my_first_car: car = ("I don't have a name", 5);

    my_second_car: mutable car;
    car.name = "I also don't have a name";
    car.size = 5;
}
