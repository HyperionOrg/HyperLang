function main() -> int32
{
	print(1 as int8 + 1 as int8);
	print(1 as int16 + 1 as int16);
	print(1 as int32 + 1 as int32);
	print(1 as int64 + 1 as int64);
	print(1 as uint8 + 1 as uint8);
	print(1 as uint16 + 1 as uint16);
	print(1 as uint32 + 1 as uint32);
	print(1 as uint64 + 1 as uint64);

	let variable_01: mutable int8 = 0 as int8;
	while (variable_01 < 10 as int8)
	{
		print(variable_01);
		++variable_01;
	}

	return 0;
}
